{
  "_version" : "1.12.0",
  "sap.app":{
    "id" : "in.sijas.ui5.app",
    "type" : "application",
    "i18n" : "i18n/i18n.properties",
    "title" : "{{pageTitle}}",
    "description" : "{{description}}",
    "applicationVersion" :{
      "version" : "1.0.0"
    }
  },
  "sap.ui":{
    "technology" : "UI5",
    "deviceTypes" : {
      "desktop" : true,
      "phone" : true,
      "tablet" : true
    }

  },
  "sap.ui5":{
    "rootView": {
      "viewName": "in.sijas.ui5.app.view.LandingPage",
      "type": "XML",
      "id": "app"
    },
    "dependencies" : {
      "minVersion" : "1.93",
      "libs":{
        "sap.m" : {},
        "sap.ui.core" : {}
      }
    },
    "models" : {
      "i18n" : {
        "type" : "sap.ui.model.resource.ResourceModel",
        "settings" : {
          "bundleName" : "in.sijas.ui5.app.i18n.i18n",
          "supportedLocales" :[""],
          "fallbackLocale" : ""
        }
      }
    }
  }
}